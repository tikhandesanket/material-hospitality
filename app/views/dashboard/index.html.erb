<h1><%=t :user_list %></h1>
<table class="table">
  <thead class="thead-light">
  <tr>
     <th scope="col">Sr.Num</th>
    <th scope="col"><%=t :first_name %></th>
    <th scope="col"><%=t :last_name %></th>
    <th scope="col"><%=t :mobile_num %></th>
    <th scope="col"><%=t :mobile_num2 %></th>
    <th scope="col"><%=t :user_type %></th>
    <th scope="col"><%=t :avg_ratings %></th>
    <th scope="col"><%=t :edit_user %></th
  </tr>
</thead>
<tbody>
  <% @users.each_with_index do |user,index| %>
    <tr>
      <th scope="row"><%=index+1%></th>
      <td><%= link_to user.first_name, user_show_path(:user_id=>user.id), :class => 'navbar-link' %> </td>
      <td><%=user.last_name %></td>
      <td><%=user.mobile_num %></td>
      <td><%=user.mobile_num2 %></td>
      <td><%=user.user_type %></td>
      <td>*****</td>
      <%user_link = (t :edit_user)   %>
      <td><%= link_to user_link, edit_user_registration_path, :class => 'navbar-link' %> </td>

    </tr>
  <% end %>
  </tbody>
</table>


